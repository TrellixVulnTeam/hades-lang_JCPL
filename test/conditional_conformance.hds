import libc as c;

def main(): Void {
    Box(true).print();
    print(Box(false));
}

struct Box[T] {
    val value: T;
}
implement Print for Bool {
    def print(this: Bool): Void {
        if this {
            c.puts(b"true");
        } else {
            c.puts(b"false");
        }
    }
}
implement[T: Print] Print for Box[T] {
    def print(this: Box[T]): Void {
        c.puts(b"Box(");
        print(this.value);
        c.puts(b")");
    }
}
interface Print {
    def print(this: This): Void;
}

def print[T: Print](value: T): Void {
    value.print();
}


