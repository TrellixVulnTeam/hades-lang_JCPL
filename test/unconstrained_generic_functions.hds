extern def puts(*u8): Void = puts
def main(): Void {
    puts(id(b"id"));
    puts(first(b"first", true));
    puts(second(false, b"second"));
    puts(second(true, StringPair(b"third", b"fourth")).first);
    fourth(true);
}

def id[T](value: T): T {
    val copy: T = value;
    return copy;
}

def first[A, B](first: A, second: B): A {
    return first;
}

def second[A, B](first: A, second: B): B {
    return second;
}

def fourth[T](value: T): T {
    puts(id(b"fourth"));
    val copy = id(value);
    puts(id(StringPair(b"fifth", b"sixth")).first);
    return value;
}

struct StringPair {
    val first: *Byte;
    val second: *Byte;
}