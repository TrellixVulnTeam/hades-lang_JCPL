
struct Box[T] {
    val value: T;
}
def main(): Void {
    val b = true

    print_bool(ref b)

    val box = Box(b"message")
    val box_ref = ref box
    val message_ref = ref box.value
    puts(message_ref)
    puts(box_ref.value)
}

extern def puts(*Byte): Void = puts

def print_bool(value: ref Bool): Void {
    if *value {
        puts(b"true")
    } else {
        puts(b"false")
    }
}